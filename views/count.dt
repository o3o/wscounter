doctype html
html
   head
      title  WebCounter Count
      link(rel='stylesheet', href='/css/main.css')
      script(src="scripts/websocket.js")
   body(onload='connect2()')
      h1#x
      h2 count
      p#counter
      - for (int i = 0; i < 8; ++i)
         button(onclick='setDgt(#{i})')set #{i}

      p
         button(onclick='start()')start

